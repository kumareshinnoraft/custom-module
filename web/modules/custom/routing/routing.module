<?php

/**
 * @file
 * Primary module hooks for routing module.
 */

/**
 * Implements hook_my_module_add_numbers().
 */
function routing_add_numbers($number1, $number2) {
  // Perform the addition operation.
  $result = $number1 + $number2;

  // Return the result.
  return $result;
}

